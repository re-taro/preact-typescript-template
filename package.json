{
  "name": "preact-typescript-template",
  "version": "1.0.0",
  "description": "Template for personal Preact projects",
  "keywords": [
    "preact",
    "typescript",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/preact-typescript-template"
  },
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://www.re-taro.dev"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite",
    "start": "vite preview",
    "lint": "eslint ./**/**/**/**/*.{js,jsx,ts,tsx,json,yaml,yml,toml}",
    "lint:fix": "npm run lint --fix",
    "fmt": "prettier -w -u .",
    "type": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "preact": "^10.6.6",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@preact/preset-vite": "2.3.0",
    "@types/node": "17.0.21",
    "autoprefixer": "10.4.7",
    "eslint": "8.18.0",
    "eslint-config-re-taro": "1.1.3",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "rollup": "2.75.7",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-plugin-windicss": "1.8.5"
  }
}
